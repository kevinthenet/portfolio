{
  "name": "kevin-castro-portfolio",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "npm run test:cypress",
    "test:spec": "npx cypress run -C cypress.config.js --spec",
    "test:interactive": "npx cypress open -C cypress.config.js",
    "test:cypress": "npx cypress run -C cypress.config.js",
    "lint:check": "npx prettier --check . --config .prettierrc.json",
    "lint:fix": "npx prettier --write . --config .prettierrc.json",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install",
    "reset": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "astro": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "cypress": "^13.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3"
  }
}
